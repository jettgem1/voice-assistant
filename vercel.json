{
  "redirects": [
    {
      "source": "/",
      "destination": "/automate",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/automate/(.*)",
      "headers": [
        {
          "key": "x-robots-tag",
          "value": "noindex"
        }
      ]
    }
  ],
  "domains": [
    "jettgemmer.com"
  ],
  "rewrites": [
    {
      "source": "/automate/:path*",
      "destination": "https://voice-assistant-mu.vercel.app/:path*"
    }
  ]
}